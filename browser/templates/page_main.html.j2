{% extends "base.html.j2" %}

{% block content %}
<div id="container">
    {% include "frag_aside.html.j2" %}

    <main id="main">
        <table>
        <thead>
            <tr>
            {% for field_name in field_names %}
                <th>{{ field_name }}</th>
            {% endfor %}
            </tr>
        </thead>
        <tbody>
            {% for row in rows %}
            <tr>
            {% for field in row %}
                <td>{{ field }}</td>
            {% endfor %}
            </tr>
            {% endfor %}
        </tbody>
        </table>
    </main>
</div>
{% endblock %}
