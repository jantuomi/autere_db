<table id="results">
<thead>
    <tr>
    {% for field_name in field_names %}
        <th>{{ field_name }}</th>
    {% endfor %}
    </tr>
</thead>
<tbody>
    {% for row in rows %}
    <tr>
    {% for field in row %}
        <td>{{ field }}</td>
    {% endfor %}
    </tr>
    {% endfor %}
</tbody>
</table>
